{
  "name": "faros-cicd-github-action",
  "version": "0.0.1",
  "private": true,
  "description": "Faros CI/CD Github Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": "https://github.com/faros-ai/emit-cicd-info-to-faros",
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@types/json-bigint": "^1.0.0",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "json-bigint": "^1.0.0",
    "pino": "^6.11.3",
    "pino-pretty": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/pino": "^6.3.8",
    "@typescript-eslint/parser": "^4.17.0",
    "@vercel/ncc": "^0.30.0",
    "eslint": "^7.23.0",
    "eslint-plugin-github": "^4.1.2",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "nock": "^13.1.0",
    "node-notifier": ">=9.0.1",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.3"
  }
}
